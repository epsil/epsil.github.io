{
  "name": "blog",
  "repository": {
    "type": "git",
    "url": "https://github.com/epsil/epsil.github.io.git"
  },
  "scripts": {
    "clean": "git stash && git clean -d -f -f -x",
    "clean!": "npm run reset && npm run clean",
    "clean!!": "npm run reset! && npm run clean",
    "ds_store": "find . -iname \"*.DS_Store\" -exec git rm {} \\;; find . -iname \"*.directory\" -exec git rm {} \\;; find . -iname \"*er i konflikt*\" -exec git rm {} \\;; find . -iname \"*.DS_Store\" -exec rm {} \\;; find . -iname \"*.directory\" -exec rm {} \\;; find . -iname \"*.log\" -exec rm {} \\;",
    "dotfiles": "npm run ds_store",
    "rebuild": "npm run build && npm run bundle && npm run build",
    "retest": "npm run clean! && npm install && npm test",
    "build": "node _/js/lib/build.js",
    "compile": "node _/js/lib/build.js index.md test.html",
    "browser": "browserify _/js/lib/index.js -o _/js/wiki.js && browserify _/js/test/index.js -o _/js/test/test.js",
    "bundle": "npm run browser && npm run lint",
    "minify": "uglifyjs js/wiki.js -o js/wiki.js",
    "http": "http-server -p 8000",
    "http2": "http-server -p 4567",
    "live": "live-server",
    "start": "npm run open && npm run server",
    "server": "live-server --port=8000 --ignore=npm-debug.log --no-browser",
    "view": "open http://localhost:8000/",
    "open": "open http://localhost:8000/",
    "append": "sh _/bin/append.sh",
    "new": "sh _/bin/new.sh",
    "edit": "sh _/bin/edit.sh",
    "note": "sh _/bin/note.sh",
    "notes": "npm run edit note",
    "export": "sh _/bin/export.sh",
    "test": "npm run spec && npm run lint",
    "prettier-css": "prettier --tab-width 4 --write \"_/{css,fonts,img}/**/*.css\"",
    "prettier-js": "prettier --single-quote --write \"_/js/{lib,templates,test,json}/**/*.{js,jsx,json}\"",
    "prettier": "npm run prettier-js && npm run prettier-css && npm run lint",
    "lint": "eslint _/js/lib && jshint _/js/lib",
    "lint2": "jshint _/js/lib/abbrev.js _/js/lib/anchor.js _/js/lib/build.js _/js/lib/collapse.js _/js/lib/footnotes.js _/js/lib/markdown.js _/js/lib/compile.js _/js/lib/figure.js _/js/lib/page.js _/js/lib/preprocessor.js _/js/lib/reference.js _/js/lib/section.js _/js/lib/social.js _/js/lib/sort.js _/js/lib/toc.js _/js/lib/util.js _/js/lib/index.js _/js/test/index.js && standard _/js/lib/abbrev.js _/js/lib/anchor.js _/js/lib/build.js _/js/lib/collapse.js _/js/lib/footnotes.js _/js/lib/markdown.js _/js/lib/compile.js _/js/lib/figure.js _/js/lib/page.js _/js/lib/preprocessor.js _/js/lib/reference.js _/js/lib/section.js _/js/lib/social.js _/js/lib/sort.js _/js/lib/toc.js _/js/lib/util.js _/js/lib/index.js _/js/test/index.js",
    "markdownlint": "markdownlint **/index.md",
    "phantom": "mocha-phantomjs _/js/test/index.html",
    "spec": "npm run bundle && open _/js/test/index.html",
    "standalone": "inliner index.html > standalone.html",
    "commit": "git commit -a -m \"Commit\" && git --no-pager status",
    "commit!": "git --no-pager status && git --no-pager diff && git commit -a -m \"Edited offline\"",
    "commit!!": "npm run save",
    "comit": "npm run commit",
    "commmit": "npm run commit",
    "commt": "npm run commit",
    "ommit": "npm run commit",
    "omit": "npm run commit",
    "ocmmit": "npm run commit",
    "update": "git --no-pager status && git --no-pager diff && git commit -a -m \"Updated `git status | sed -n '/modified:/ {s/^[[:space:]]*modified:[[:space:]]*//;s/\\.txt$//;s/\\.md$//;s/\\.html$//;s/\\/index$//;p;q;}'`\"",
    "save": "npm run commit && npm run push",
    "amend": "git --no-pager status && git --no-pager diff && git commit -a --amend --no-edit",
    "amend-html": "npm run build && npm run amend",
    "build-amend": "npm run rebuild && npm run amend",
    "reword": "git --no-pager status && git --no-pager diff && git commit -a --amend",
    "reset": "git reset --hard",
    "clear": "git stash clear",
    "reset!": "npm run reset && npm run clear",
    "push": "git --no-pager status && git pull --rebase && git push",
    "push!": "git --no-pager status && git push --force",
    "pull": "git --no-pager status && git pull --rebase",
    "compress": "git gc",
    "compress!": "git gc --prune=now",
    "log": "git log -p",
    "gitk": "gitk &",
    "gui": "git gui &",
    "status": "git status",
    "system": "neofetch && git status",
    "grep": "sh _/bin/grep.sh",
    "todo": "sh _/bin/todo.sh",
    "search": "npm run grep",
    "deploy": "npm run sync",
    "docx": "for f in **/index.md; do (cd \"${f%index.md}\" && pwd && pandoc -S -f markdown -t docx -o \"index.docx\" \"index.md\"); done",
    "encrypt": "sh _/bin/encrypt.sh",
    "decrypt": "sh _/bin/decrypt.sh --gui",
    "decrypt-term": "sh _/bin/decrypt.sh",
    "decrypt-terminal": "npm run decrypt-term",
    "reencrypt": "npm run decrypt"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "clipboard": "^1.7.1",
    "datatables": "^1.10.18",
    "glob": "^7.0.6",
    "gray-matter": "^2.0.2",
    "handlebars": "^4.0.5",
    "jquery": "^3.1.0",
    "jsdom": "^9.4.1",
    "lodash": "^4.17.5",
    "markdown-it": "^7.0.0",
    "markdown-it-abbr": "^1.0.3",
    "markdown-it-attrs": "^2.3.1",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.1",
    "markdown-it-emoji": "^1.3.0",
    "markdown-it-footnote": "^3.0.0",
    "markdown-it-implicit-figures": "^0.3.2",
    "markdown-it-mathjax": "^2.0.0",
    "markdown-it-prism": "^1.1.2",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-checkbox": "^1.0.3",
    "md5": "^2.1.0",
    "moment": "^2.18.1",
    "openpgp": "^2.5.11",
    "string": "^3.3.1",
    "string-similarity": "^1.2.0",
    "tidy-html5": "^0.1.1",
    "typogr": "^0.6.6",
    "urijs": "^1.18.1"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "eslint": "^5.6.1",
    "http-server": "^0.9.0",
    "inliner": "^1.10.2",
    "jshint": "^2.9.3",
    "live-server": "^1.2.0",
    "markdownlint-cli": "^0.2.0",
    "prettier": "^1.14.3",
    "standard": "^8.0.0"
  }
}
